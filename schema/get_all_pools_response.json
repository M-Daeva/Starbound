  ],
  "properties": {
    "all_pools": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Pool"
      }
    }
  },
  "definitions": {
    "Pool": {
      "type": "object",
      "required": [
        "number",
        "symbol_first",
        "symbol_second"
      ],
      "properties": {
        "number": {
          "type": "integer",
          "format": "uint128",
          "minimum": 0.0
        },
        "symbol_first": {
          "type": "string"
        },
        "symbol_second": {
          "type": "string"
        }
      }
    }
  }
}
