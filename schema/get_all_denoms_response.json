  ],
  "properties": {
    "all_assets_info": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/AssetInfo"
      }
    }
  },
  "definitions": {
    "AssetInfo": {
      "type": "object",
      "required": [
        "asset_denom",
        "asset_symbol",
        "price"
      ],
      "properties": {
        "asset_denom": {
          "type": "string"
        },
        "asset_symbol": {
          "type": "string"
        },
        "price": {
          "type": "integer",
          "format": "uint128",
          "minimum": 0.0
        }
      }
    }
  }
}
